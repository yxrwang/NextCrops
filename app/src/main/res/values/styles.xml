<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimary</item>
    </style>

    <style name="VicButton" parent="Widget.AppCompat.Button.Colored">
        <item name="colorButtonNormal">@color/colorPrimary</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>

    <style name="QueensLandButton" parent="Widget.AppCompat.Button.Colored">
        <item name="colorButtonNormal">@color/colorQueensland</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>

    <style name="MenuButton" parent="Widget.AppCompat.Button.Borderless">

        <item name="android:textColor">#FFFFFF</item>

    </style>

    <style name="QueensLandMenuButton" parent="MenuButton">

        <item name="colorButtonNormal">@color/colorQueensland</item>
    </style>

</resources>
